package application;

public class O_Flugzeug {
	private int id, sitze;
	private String modell;

//---Konstruktor
	public O_Flugzeug() {}
	public O_Flugzeug(int id, String modell, int sitze) {
		this.id = id;
		this.modell = modell;
		this.sitze = sitze;		
	}
//---Get-Set	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getModell() {
		return modell;
	}
	public void setModell(String modell) {
		this.modell = modell;
	}
	public int getSitze() {
		return sitze;
	}
	public void setSitze(int sitze) {
		this.sitze = sitze;
	}
	
//---Methoden	
	
	
	
}
