package application;

public class helper {
	private String look, save;
	int id;
	
	public helper() {}
	public helper(int id) {
		this.id=id;
	}
	public helper(String look, String save, int id) {
		this.look=look;
		this.save=save;
		this.id=id;
	}

	public int getID() {
		return id;
	}
	public void setLook(String look) {
		this.look=look;
		System.out.println("look="+look);
	}
	public void setSave(String save) {
		this.save=save;
		System.out.println("save="+save);
	}
	
	public String getLook() {
		System.out.print("look="+look);
		return look;
	}
	public String getSave() {
		return save;
	}
	
}
